<?xml version="1.0" encoding="utf-8"?>
<install type="acesef_ext" version="1.5.0" method="upgrade">
	<name>AceSEF</name>
	<author>JoomAce LLC</author>
	<creationDate>April 2012</creationDate>
	<copyright>(C) 2009-2012 JoomAce LLC</copyright>
	<license>Combined License, http://www.joomace.net/company/license</license>
	<authorEmail>info@joomace.net</authorEmail>
	<authorUrl>www.joomace.net</authorUrl>
	<version>1.5.1</version>
	<description>AceSEF extension for AceSEF.</description>
	<files>
		<filename acesef_ext="com_acesef">com_acesef.php</filename>
	</files>
	<params group="url">
        <param name="tag_prefix" type="text" default="" label="Tag URL prefix" description="Tag URL prefix" />
		<param name="tag_part" type="list" default="global" label="ACESEF_PARAMS_URL_PART_TAG" description="ACESEF_PARAMS_URL_PART_TAG">
 			<option value="global">Use Global</option>
 			<option value="title">ACESEF_COMMON_TITLE_FIELD</option>
 			<option value="alias">ACESEF_COMMON_ALIAS_FIELD</option>
 		</param>
		<param name="tagid_inc" type="radio" default="1" label="ACESEF_PARAMS_URL_TAG_ID" description="ACESEF_PARAMS_URL_TAG_ID_HELP">
 			<option value="1">No</option>
 			<option value="2">Yes</option>
 		</param>
 	</params>
</install>