<?xml version="1.0" encoding="utf-8"?>
<install type="acesef_ext" version="1.5.0" method="upgrade">
	<name>Mail To</name>
	<author>JoomAce LLC</author>
	<creationDate>June 2010</creationDate>
	<copyright>(C) 2009-2010 JoomAce LLC</copyright>
	<license>GNU/GPL http://www.gnu.org/copyleft/gpl.html</license>
	<authorEmail>info@joomace.net</authorEmail>
	<authorUrl>www.joomace.net</authorUrl>
	<version>1.5.1</version>
	<description>Mail To extension for AceSEF.</description>
	<files>
		<filename acesef_ext="com_mailto">com_mailto.php</filename>
	</files>
	<params group="url">
		<param name="remove_url_suffix" type="radio" default="0" label="Remove URL suffix" description="Remove URL suffix">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
	</params>
	<install>
		<defaultParams>
			<defaultParam name="prefix" value="mail" />
		</defaultParams>
	</install>
</install>